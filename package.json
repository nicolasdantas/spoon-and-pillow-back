{
  "name": "express-with-ts",
  "version": "1.0.0",
  "description": "Node templace using TS",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "tsc",
    "lint": "eslint . --ext .ts & echo 'lint complete'",
    "lint-fix": "eslint --fix --ext .ts ",
    "dev": "ts-node-dev --transpile-only --respawn  ./src/index.ts",
    "prod": "tsc && node ./build/src/index.js",
    "debug": "tsc && node --inspect ./build/src/index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.12.5",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
